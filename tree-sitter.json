{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "tek",
      "camelcase": "Tek",
      "title": "Tek",
      "scope": "source.tek",
      "file-types": ["tek", "ct"],
      "highlights": ["queries/highlights.scm"],
      "injection-regex": "^tek$",
      "class-name": "TreeSitterTek"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "The tek markup language used for Teksty",
    "authors": [
      {
        "name": "Allomanta"
      }
    ],
    "links": {
      "repository": "https://github.com/allomanta/tree-sitter-tek"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
